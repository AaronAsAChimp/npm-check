{
  "name": "npm-check",
  "version": "5.4.0",
  "description": "Check for outdated, incorrect, and unused dependencies.",
  "main": "lib",
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rm -rf lib && babel src --out-dir lib",
    "flow": "flow check",
    "flow-typed": "flow-typed install",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "spec": "npm run build && npm link && jest",
    "test": "npm run lint && npm run flow && npm run spec",
    "version": "npm run build",
    "watch": "babel src --out-dir lib --watch"
  },
  "bin": {
    "npm-check": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylang/npm-check.git"
  },
  "keywords": [
    "npm",
    "outdated",
    "dependencies",
    "unused",
    "changelog",
    "check",
    "updates",
    "api",
    "interactive",
    "cli",
    "safe",
    "updating",
    "updater",
    "installer",
    "devDependencies"
  ],
  "author": {
    "name": "Dylan Greene",
    "email": "dylang@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dylang/npm-check/issues"
  },
  "homepage": "https://github.com/dylang/npm-check",
  "files": [
    "bin",
    "lib"
  ],
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "callsite-record": "^3.2.2",
    "chalk": "^1.1.3",
    "co": "^4.6.0",
    "depcheck": "^0.6.7",
    "execa": "^0.6.0",
    "giturl": "^1.0.0",
    "global-modules": "^0.2.3",
    "globby": "^6.1.0",
    "inquirer": "^3.0.4",
    "is-ci": "^1.0.10",
    "lodash": "^4.17.4",
    "meow": "^3.7.0",
    "merge-options": "0.0.64",
    "minimatch": "^3.0.3",
    "node-emoji": "^1.5.1",
    "ora": "^1.1.0",
    "package-json": "^3.1.0",
    "path-exists": "^3.0.0",
    "pkg-dir": "^1.0.0",
    "semver": "^5.3.0",
    "semver-diff": "^2.1.0",
    "text-table": "^0.2.0",
    "throat": "^3.0.0",
    "update-notifier": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.39.0",
    "flow-typed": "^2.0.0",
    "jest-cli": "^19.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
